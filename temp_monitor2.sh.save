t_sdd=$(hddtemp -n /dev/sdd) t_sde=$(hddtemp -n /dev/sde) t_sdf=$(hddtemp -n /dev/sdf) t_sdg=$(hddtemp -n /dev/sdg) t_sdg=$(hddtemp -n /dev/sdg) echo -e "/dev/sda: $t_sda\n"


